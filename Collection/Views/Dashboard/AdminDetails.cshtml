@model incasso.Administrators.dto.AdministratorDto
@{
    Layout = null;
}

<div class="row">
    <div class="col s12 l6">
        <div class="row">
            <div class="col s12">
                <i class="material-icons left">label</i><span class="semibold">@L("Number"):</span>
                @Model.Number
            </div>
            <div class="col s12">
                <i class="material-icons left">person</i><span class="semibold">@L("Name"):</span>
                @Model.Name
            </div>
            <div class="col s12">
                <i class="material-icons left">person_pin</i><span class="semibold">@L("Contactperson"):</span>
                @Model.Contact
            </div>
            <div class="col s12">
                <i class="material-icons left">phone</i><span class="semibold">@L("Telefoonnummer"):</span>
                @Model.Phone
            </div>
            <div class="col s12">
                <i class="material-icons left">email</i><span class="semibold">@L("Email"):</span>
                @Model.Email
            </div>
        </div>
    </div>
    <div class="col s12 l6">
        <div class="row">
            <div class="col s12">
                <i class="material-icons left">account_balance</i><span class="semibold">@L("Bank"):</span>
                @Model.Bank
            </div>
            <div class="col s12">
                <i class="material-icons left">account_balance</i><span class="semibold">@L("Bankrekening"):</span>
                @Model.Account
            </div>
            <div class="col s12">
                <i class="material-icons left">account_balance</i><span class="semibold">@L("IBAN"):</span>
                @Model.Iban
            </div>
            <div class="col s12">
                <i class="material-icons left">account_balance</i><span class="semibold">@L("BIC"):</span>
                @Model.Bic
            </div>
        </div>
    </div>
</div>